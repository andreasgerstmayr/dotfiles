[global_config]
  enabled_plugins = ActivityWatch, InactivityWatch
[keybindings]
  paste = <Primary>v
[profiles]
  [[default]]
    background_color = "#110f1d"
    background_image = None
    font = Consolas 13
    foreground_color = "#f2f2f2"
    show_titlebar = False
    scrollback_lines = 5000
