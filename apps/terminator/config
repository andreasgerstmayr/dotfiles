    use_custom_command = True
    custom_command = TERM=xterm-256color zsh
    use_system_font = False
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
  [[InactivityWatch]]
    inactive_period = 600
